  "repository": "https://github.com/iloveupal/classical-music-crud-tool.git",
  "author": "Danil Sorokin <d.sorokin.msk@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "@loadable/component": "^5.7.0",
    "asyncbox": "^2.5.2",
    "boom": "^7.3.0",
    "express": "^4.16.4",
    "express-joi-validator": "^2.0.1",
    "joi": "^14.3.1",
    "lodash": "^4.17.11",
    "mongodb": "^3.2.3"
  },
  "scripts": {
    "server:dev": "webpack --mode development --config webpack.server.config.js",
    "client:dev": "webpack --mode development --config webpack.config.js",
    "dev": "rm -rf dist && concurrently --kill-others \"yarn server:dev\" \"yarn client:dev\"",
    "prod": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.config.prod.js",
    "start:dev": "nodemon --inspect ./dist/server.js",
    "start:prod": "node ./dist/server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^1.18.11",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
